{{!
    @template block_testblock/testblock
    Example context (json):
    {
        "modid": "9",
        "modname": "test",
        "modcreated": "12 jan 2022",
        "completionstatus": "completed"
    }
}}
<div>
    <div class="testblock">
        <div class="mod-name">
         {{#modules}}
         <ul>
         <li>
            <label id="mod-name">{{modid}}- <a href="{{mod_url}}" class="">{{modname}}</a>-{{modcreated}}-{{completionstatus}}</label>
            </li>
            </ul>
            {{/modules}}
        </div>
    </div>
</div>
